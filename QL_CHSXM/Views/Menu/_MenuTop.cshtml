@model IEnumerable<QL_CHSXM.Models.EF.Category>

@if (Model != null && Model.Any())
{
<ul class="navbar_menu" style="font-family:Roboto; font-size: 3rem">
    @foreach (var item in Model)
    {
        if (item.Alias.Equals("trang-chu"))
        {
            item.Alias = "";
        }
        if (!string.IsNullOrEmpty(item.Link))
        {
            item.Alias = item.Link;
        }
        <li><a href="/@item.Alias">@item.Title</a></li>
    }
</ul>
}